{
	"name": "dokan/try-aura",
	"description": "an AI-powered virtual try-on and content creation platform built for fashion and eCommerce.",
	"type": "wordpress-plugin",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Dokan\\TryAura\\": "inc/"
		}
	},
	"authors": [
		{
			"name": "Dokan Inc.",
			"email": "info@dokan.co"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"automattic/vipwpcs": "^3.0",
		"phpcompatibility/phpcompatibility-wp": "*",
		"wp-coding-standards/wpcs": "^3.0",
		"squizlabs/php_codesniffer": "*",
		"dealerdirect/phpcodesniffer-composer-installer": "*"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"scripts": {
		"phpcs": [
			"vendor/bin/phpcs inc templates try-aura.php --standard=phpcs.xml"
		],
		"phpcs:report": [
			"vendor/bin/phpcs inc templates try-aura.php --standard=phpcs.xml --report-file='phpcs-report.txt'"
		],
		"phpcbf": [
			"vendor/bin/phpcbf -p"
		]
	}
}
